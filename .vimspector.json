{
    "configurations" : {
        "Launch" : {
            "adapter" : "CodeLLDB",
            "filetypes" : [ "c", "cpp", "rust" ],
            "configuration" : {
                "request": "launch",
                "program": "build/thuscnet",
                "externalConsole": true,
            }
        }
    }
    "Attach" : {
        "adapter" : "CodeLLDB",
        "filetypes" : [ "c", "cpp", "rust" ],
        "configuration" : {
            "request": "attach",
            "program": "build/thuscnet",
            "processId": "${input:processId}",
            "externalConsole": true,
        }
    },
}
